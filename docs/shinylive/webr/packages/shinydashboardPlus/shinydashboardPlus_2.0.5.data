<span id="cb2-13"><a href="#cb2-13" tabindex="-1"></a>          ),</span>
<span id="cb2-14"><a href="#cb2-14" tabindex="-1"></a>          <span class="fu">prettyToggle</span>(</span>
<span id="cb2-15"><a href="#cb2-15" tabindex="-1"></a>            <span class="at">inputId =</span> <span class="st">&quot;na&quot;</span>,</span>
<span id="cb2-16"><a href="#cb2-16" tabindex="-1"></a>            <span class="at">label_on =</span> <span class="st">&quot;NAs kept&quot;</span>,</span>
<span id="cb2-17"><a href="#cb2-17" tabindex="-1"></a>            <span class="at">label_off =</span> <span class="st">&quot;NAs removed&quot;</span>,</span>
