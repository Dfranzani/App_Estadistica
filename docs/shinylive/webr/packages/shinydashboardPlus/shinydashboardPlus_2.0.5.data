<span id="cb2-24"><a href="#cb2-24" tabindex="-1"></a>          <span class="at">title =</span> <span class="st">&quot;Dropdown 2&quot;</span>,</span>
<span id="cb2-25"><a href="#cb2-25" tabindex="-1"></a>          <span class="at">icon =</span> <span class="st">&quot;sliders-h&quot;</span>,</span>
<span id="cb2-26"><a href="#cb2-26" tabindex="-1"></a>          <span class="fu">prettySwitch</span>(</span>
